{
    "atom-workspace": {
        "ctrl-alt-cmd-[": "pattern-navigator:toggle",
        "ctrl-alt-cmd-]": "pattern-navigator:close",
        "ctrl-alt-cmd-up": "pattern-navigator:previousFold",
        "ctrl-alt-cmd-down": "pattern-navigator:nextFold",
        "ctrl-alt-cmd-right": "pattern-navigator:moveLevelDown",
        "ctrl-alt-cmd-left": "pattern-navigator:moveLevelUp",
        "ctrl-1": "pattern-navigator:toggleFoldsLevel1",
        "ctrl-2": "pattern-navigator:toggleFoldsLevel2",
        "ctrl-3": "pattern-navigator:toggleFoldsLevel3",
        "ctrl-4": "pattern-navigator:toggleFoldsLevel4",
        "ctrl-5": "pattern-navigator:toggleFoldsLevel5",
        "ctrl-alt-cmd-shift": "pattern-navigator:toggleSearch",
        "ctrl-alt-cmd-/": "pattern-navigator:toggleActiveFold"
    }
}
